FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];


internalField   uniform 1;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 159000;
    }
    outlet
    {
        type            fixedValue;
        value           uniform 101325;
    }
    wallUpper
    {
        type            zeroGradient;
    }
    wallSymmetricBorder
    {
        type            slip;
    }
    symwall
    {
        type            empty;
    }
}
