FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue volumeFraction1 0
    volVectorFieldValue U (0 0 0)
);

regions
(
    boxToCell
    {
        box  (0 -1 -19) (1 1 19);
        
        fieldValues
        (
            volScalarFieldValue volumeFraction1 1
            volVectorFieldValue U (0 0 0)
        );
    }

    sphereToCell
    {
        origin (0 0 -19);
        radius 1;
        
        fieldValues
        (
            volScalarFieldValue volumeFraction1 1
            volVectorFieldValue U (0 0 0)
        );
    }
    
    sphereToCell
    {
        origin (0 0 19);
        radius 1;
        
        fieldValues
        (
            volScalarFieldValue volumeFraction1 1
            volVectorFieldValue U (0 0 0)
        );
    }

);


