FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue T 308.15
    volScalarFieldValue p 1e+9
    volVectorFieldValue U ( 0 0 0 )
    volScalarFieldValue volumeFraction1 0
);

regions
(
    boxToCell
    {
        box ( 5 -1 -1 ) ( 10  1  1 );
        fieldValues
        (
            volScalarFieldValue volumeFraction1 1
            volScalarFieldValue p 1e+5
        );
    }
);


